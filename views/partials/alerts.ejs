<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
<% if (locals.messages.err) { %>
    <script>
        Swal.fire({
        title: 'Error!',
        text: '<%=locals.messages.err%>',
        icon: 'error',
        confirmButtonText: 'Okay'
    })
    </script>
<% } %>
